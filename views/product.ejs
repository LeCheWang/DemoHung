<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="css/product.css">
</head>
<body>
    <%- include("menu.ejs") %>
    <h1>
        Đây là trang Product
    </h1>

    <ul class="products">
        <% products.forEach(function(product){ %>
            <li class="product">
                <h1>
                    <%= product.name %>
                </h1>
                <img src="<%= product.img %>" alt="">
                <p>
                    <%= product.price %>
                </p>
            </li>
        <% }); %>
    </ul>



</body>
</html>